          "resources/**",
          "resources/*.png",
          "**/resources/logo.png",
          "**/resources/*.ico",
          "**/resources/*.png",
          "**/resources/*.jpg"
        ],
        "exclude": [
          "docs/**",
          "en/**",
          "es/**",
          "Alis/**"
        ]
      }
    ],
    "output": "../../es/v0.4.0",
    "template": ["templates/default", "templates/modern"],
    "globalMetadataFiles": [],
