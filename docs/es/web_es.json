          "docs/**",
          "en/**",
          "es/**",
          "Alis/**",
          "templates/**",
          "videos/**",
          "js/**",
          "images/**",
          "css/**",
          ".github/**",
          ".git/**"
        ]
      }
    ],
    "resource": [
